     <div class="bg-gray-800 rounded-lg shadow-lg p-6 flex flex-col">
      <img alt="Roblox game screenshot showing health bars displayed above enemy players' heads" class="rounded-md mb-4" height="200" src="https://storage.googleapis.com/a1aa/image/7d887d3f-b463-4bd3-e93a-434326a85fa4.jpg" width="400"/>
      <h3 class="text-xl font-semibold mb-2">
       Health Bar ESP
      </h3>
      <p class="flex-grow text-gray-300 mb-4">
       Shows health bars above enemy players to monitor their health status.
      </p>
      <pre class="bg-gray-900 p-4 rounded text-sm overflow-x-auto text-green-400 font-mono">
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local localPlayer = Players.LocalPlayer

local healthBars = {}

function createHealthBar(player)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = player.Character and player.Character:FindFirstChild("Head")
    billboard.Size = UDim2.new(0, 100, 0, 10)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local frame = Instance.new("Frame", billboard)
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    frame.BorderSizePixel = 0

    local healthBar = Instance.new("Frame", frame)
    healthBar.Size = UDim2.new(1, 0, 1, 0)
    healthBar.BackgroundColor3 = Color3.new(0, 1, 0)
    healthBar.BorderSizePixel = 0

    billboard.Parent = player.Character

    local humanoid = player.Character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.HealthChanged:Connect(function()
            local ratio = humanoid.Health / humanoid.MaxHealth
            healthBar.Size = UDim2.new(ratio, 0, 1, 0)
            if ratio &gt; 0.5 then
                healthBar.BackgroundColor3 = Color3.new(0, 1, 0)
            elseif ratio &gt; 0.2 then
                healthBar.BackgroundColor3 = Color3.new(1, 1, 0)
            else
                healthBar.BackgroundColor3 = Color3.new(1, 0, 0)
            end
        end)
    end

    return billboard
end

Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        wait(1)
        if healthBars[player] then
            healthBars[player]:Destroy()
            healthBars[player] = nil
        end
        if player ~= localPlayer then
            healthBars[player] = createHealthBar(player)
        end
    end)
end)

Players.PlayerRemoving:Connect(function(player)
    if healthBars[player] then
        healthBars[player]:Destroy()
        healthBars[player] = nil
    end
end)
          </pre>
